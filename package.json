{
  "name": "ulti-lobby",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "jest",
    "pack": "electron-builder --dir",
    "build": "electron-builder -wl --x64 --publish never"
  },
  "devDependencies": {
    "electron-builder": "22.9.1",
    "electron": "^12.0.1",
    "jest": "^26.2.2"
  },
  "dependencies": {
    "discord-rpc": "^3.1.4",
    "electron-store": "^6.0.1",
    "messenger": "0.0.9"
  },
  "build": {
    "win": {
      "target": [
        "portable"
      ],
      "asar": true,
    "asarUnpack": [
      "assets"
      
    ]},
    

    "portable": {
      "artifactName": "lobby.exe"
    }
    
  }
  
}


